{
  "comments": {
    "blockComment": ["\"", "\""]
  },
  "brackets": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"]
  ],
  "autoClosingPairs": [
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
    { "open": "{", "close": "}" },
    { "open": "'", "close": "'", "notIn": ["string"] },
    { "open": "\"", "close": "\"", "notIn": ["comment"] }
  ],
  "surroundingPairs": [
    ["[", "]"],
    ["(", ")"],
    ["{", "}"],
    ["'", "'"],
    ["\"", "\""]
  ],
  "folding": {
    "markers": {
      "start": "\\[",
      "end": "\\]"
    }
  },
  "indentationRules": {
    "increaseIndentPattern": "[\\[\\{]\\s*$",
    "decreaseIndentPattern": "^\\s*[\\]\\}]"
  }
}
